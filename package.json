{
  "name": "chalcogen",
  "version": "0.4.0",
  "description": "Runs mocha tests using selenium",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "main": "./lib/chalcogen.js",
  "bin": {
    "chalcogen": "./bin/chalcogen.js"
  },
  "engineStrict": true,
  "engines": {
    "npm": ">= 1.3.8",
    "node": ">= 0.10.17"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jakobmattsson/chalcogen.git"
  },
  "scripts": {
    "pretest": "coffee -co lib src",
    "prepublish": "coffee -co lib src",
    "coverage": "jscov src .cov && JSCOV=.cov mocha --compilers coffee:coffee-script --recursive --reporter mocha-term-cov-reporter spec",
    "test": "mocha --compilers coffee:coffee-script --recursive spec"
  },
  "peerDependencies": {
    "opra": ">= 0.17.2"
  },
  "dependencies": {
    "nconf": "^0.6.7",
    "request": "^2.34.0",
    "underscore": "^1.5.1",
    "random-name": "^0.1.0",
    "saucelabs": "^0.1.0",
    "runr": "^0.4.3",
    "wd": "^0.1.0",
    "async": "^0.2.9",
    "optimist": "^0.6.0"
  },
  "devDependencies": {
    "coffee-script": "^1.6.3",
    "jscov": "^0.5.7",
    "mocha": "^1.12.0",
    "mocha-term-cov-reporter": "^0.1.4"
  }
}
